{
  "name": "emr-electron-ts",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run start:react\" \"npm run start:electron\"",
    "start:react": "BROWSER=none react-scripts start",
    "start:electron": "wait-on http://localhost:3000 && electron . --dev",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build": "react-scripts build && npm run build:electron",
    "package": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.emr.desktop",
    "files": [
      "dist/**/*",
      "build/**/*"
    ],
    "directories": {
      "output": "release"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/sqlite3": "^3.1.11",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5",
    "wait-on": "^8.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
