{
  "name": "emr-electron-ts",
  "version": "1.0.0",
  "main": "dist/main/main.js",
    "scripts": {
      "start": "npm run build && electron .",
      "build": "tsc -p tsconfig.electron.json && cp -r public dist/public",
      "dev": "concurrently \"npm run start:react\" \"npm run build:electron && wait-on http://localhost:3000 && ts-node src/electron/main.ts\"",
      "start:react": "react-scripts start",
      "package": "npm run build:react && npm run build:electron && electron-builder --mac --win --linux",
      "build:react": "react-scripts build",
      "build:electron": "tsc -p tsconfig.electron.json"
    },
    "build": {
      "appId": "com.emr.desktop",
      "files": [
        "dist/**/*",
        "build/**/*"
      ],
      "directories": {
        "output": "release"
      }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
      "axios": "^1.8.1",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "sqlite3": "^5.1.7",
      "react-scripts": "5.0.1"
    },
    "devDependencies": {
      "@types/node": "^22.13.9",
      "@types/react": "^19.0.10",
      "@types/react-dom": "^19.0.4",
      "@types/sqlite3": "^3.1.11",
      "concurrently": "^9.1.2",
      "electron": "^35.0.0",
      "electron-builder": "^25.1.8",
      "ts-node": "^10.9.2",
      "typescript": "4.9.5",
      "wait-on": "^8.0.2"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }